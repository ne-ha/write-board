<%= form_for(@note) do |f| %>
  <table>
    <tr>
      <th><%= f.label(:title, "Title") %></th>
      <td><%= f.text_field(:title) %></td>
    </tr>
    <tr>
      <th><%= f.label(:description, "Description") %></th>
      <td><%= f.text_field(:description) %></td>
    </tr>
    <tr>
      <th><%= f.submit 'Save'%></th>
    </tr>
  </table>
<%end%>
