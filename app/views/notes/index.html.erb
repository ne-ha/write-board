<div class="btn btn-default">
    <%= link_to 'Add Notes', new_note_path %>
</div>

<ul>
  <% @notes.each do |note| %>
    <li><%= note.title %>
      <div class="btn btn-default">
        <%= link_to 'Edit', edit_note_path(note.id) %>
      </div>
      <div class="btn btn-default">
        <%= link_to 'Delete', note_path(note), :method => 'delete' %>
      </div>
      <div class="btn btn-default">
        <%= link_to 'Share', id: "share_to" , :remote=>true %>
      </div>
    </li>
  <% end %>
</ul>

<%= render 'shared/modal_share'%>
